<tool id="portal_Sff2FastQ" name="Sff2FastQ" version="1.0.0">
	<description>Convert SFF files to FastQ files</description>
	<command>sff2fastq $sff_file -o $outfile 
	</command>
	<inputs>
	<param value="" name="sff_file" optional="False" type="data" label="SFF file" />
	</inputs>
	<outputs>
		<data name="outfile" format="fastq"></data>
	</outputs>
	<help>
	</help>
</tool>
