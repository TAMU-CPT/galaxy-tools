<?xml version="1.0"?>
<tool id="unknown_app_bin/cpt_raw_seq_export.pl" name="Raw Seq Export" version="1.94">
  <description>Export the whole genomic sequence from a Genbank file</description>
  <version_command>perl bin/cpt_raw_seq_export.pl --version</version_command>
  <stdio>
    <exit_code range="1:" level="fatal"/>
  </stdio>
  <command interpreter="perl">bin/cpt_raw_seq_export.pl
--galaxy
--outfile_supporting $__new_file_path__
--file "${file}"

--fasta "${fasta}"

--fasta_files_path "${fasta.files_path}"

--fasta_format "${fasta_format}"

--fasta_id "${fasta.id}"

</command>
  <inputs>
    <param name="file" help="Input file" optional="False" label="file" type="data"/>
    <param name="fasta_format" help="Raw Sequence" optional="False" label="Format of fasta" type="select">
      <option value="Fasta" selected="True">Fasta</option>
    </param>
  </inputs>
  <outputs>
    <data name="fasta" format="Fasta">
      <change_format>
        <when input="fasta_format" value="Fasta" format="fasta"/>
      </change_format>
    </data>
  </outputs>
  <help>DESCRIPTION
===========

Export the entire genomic sequence from a GenBank file
</help>
  <tests>
    <test>
      <param name="file" value="test-data/inputs/multi.gbk"/>
      <output name="results" file="test-data/outputs/raw_export.fa"/>
    </test>
  </tests>
</tool>
